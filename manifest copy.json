{
  "name": "My First Extension",
  "version": "1.0",
  "description": "The first extension that I made.",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*"],
      "js": ["jquery-1.7.1.min.js","myscript.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },  
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com"
  ]
}