{
  "name": "ProxMate",
  "version": "1.0",
  "description": "Unblocks youtube and grooveshark :)",
  "background_page": "background.html",
  "homepage_url": "http://personalitycores.com",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["jquery-1.7.1.min.js","fx.js","youtube.js"]
    },
    {
      "matches": ["*://*.youtube.com/results*"],
      "js": ["jquery-1.7.1.min.js","fx.js","youtube-search.js"]
    },
    {
      "matches": ["*://*.youtube.com/user/*"],
      "js": ["jquery-1.7.1.min.js","fx.js","youtube-channel.js"]
    },
    {
      "matches": ["*://*.grooveshark.com/*"],
      "js": ["jquery-1.7.1.min.js","fx.js","grooveshark.js"]
    }
  ],
  "browser_action": {
    "name": "Click to enable / disable ProxMate",
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "16": "icon16.png",
    "128": "icon128.png"
   },
  "permissions": [
    "proxy"
  ]
}